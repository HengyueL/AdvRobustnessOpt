{   
    "seed": 42,
    "continue": false,
    "curr_batch": 0,
    "end_batch_num": 100,

    "log_folder":{
        "save_root": "Test-Sanity-APGD-Max"
    },

    "classifier_model":{
        "type": "DeepMind-Cifar10",
        "weight_path": "os.path.join('..', 'model', 'DeepMind-Cifar10', 'L2-Extra.pt')",
        "use_clamp_input": false
    },

    "dataset":{
        "name": "cifar10",
        "num_classes": 10,
        "label_list": "list(range(0, 10, 1))",

        "input_size": 32,
        "workers": 0,
        "batch_size": 1,
        "clean_dataset_path": "C:\\Users\\Liang\\dataset\\Cifar10\\Clean",
        "train_transform": "default"

    },

    "test_attack": {
        "attack_type": "APGD",
        "attack_true_label": true
    },

    "apgd_params":{
        "attack_method": "L1",
        "attack_bound": 12,

        "apgd_n_restart": 5,
        "apgd_max_iter": 100
    },

    "fab_params":{
        "attack_method": "L1",

        "fab_init_scale": 12,
        "fab_n_restart": 5,
        "fab_max_iter": 100
    },

    "granso_params": {
        "attack_method": "L1",
        "attack_bound": 12,

        "granso_max_iter": 400,
        "granso_early_max_iter": 20,
        "granso_early_feasibility_thres": 0.01,
        "granso_restarts": 5,
        "granso_loss": {
            "type": "Margin",
            "reduction": "sum",
            "use_clip_loss": true
        },

        "granso_input_constraint_type": "L2-folding",
        "granso_mu0": 1,
        "granso_mem": 10,
        "granso_stat_l2": false,
        "granso_steering_l1": false
    },

    "comment": {
        "cifar": "E:\\Dataset\\cifar10\\clean",
        "imagenet": "E:\\Dataset\\ImageNet",
        "imagenet_2": "C:\\Users\\Liang\\dataset\\ImageNet",
        "cifar_2": "C:\\Users\\Liang\\dataset\\Cifar10\\Clean"
    }
}
