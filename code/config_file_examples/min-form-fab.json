
{   
    "seed": 42,
    "continue": false,
    "curr_sample": 0,
    "end_sample": 2,
    "save_vis": true,

    "log_folder":{
        "save_root": "Test-Min-FAB"
    },

    "classifier_model":{
        "type": "PAT-Cifar10",
        "weight_path": "os.path.join('..', 'model', 'PAT-Cifar10', 'pat_self_0.5.pt')",
        "use_clamp_input": false
    },

    "dataset":{
        "name": "cifar10",
        "num_classes": 10,
        "label_list": "list(range(0, 10, 1))",

        "input_size": 32,
        "workers": 0,
        "dataset_path": "C:\\Users\\Liang\\dataset\\Cifar10\\Clean",
        "train_transform": "default"

    },

    "test_attack": {
        "attack_type": "FAB"
    },

    "apgd_params":{
        "distance_metric": "L2",
        "attack_bound": 0.5,

        "apgd_n_restarts": 5,
        "apgd_max_iter": 200
    },

    "fab_params":{
        "distance_metric": "L1",

        "fab_init_scale": 12,
        "fab_n_restarts": 2,
        "fab_max_iter": 100
    },

    "granso_params": {
        "distance_metric": "L2",
        "attack_bound": 0.5,
        
        "granso_early_max_iter": 20,
        "granso_max_iter": 400,
        "granso_early_feasibility_thres": 0.01,
        "granso_n_restarts": 5,

        "granso_loss": {
            "type": "Margin",
            "reduction": "sum",
            "use_clip_loss": true
        },

        "granso_input_constraint_type": "L2-folding",
        "granso_mu0": 1,
        "granso_mem": 10,
        "granso_stat_l2": false,
        "granso_steering_l1": false,

        "granso_eq_tol": 1e-4,
        "granso_ieq_tol": 1e-4,
        "granso_opt_tol": 1e-4
    },

    "comment": {
        "cifar": "E:\\Dataset\\cifar10\\clean",
        "imagenet": "E:\\Dataset\\ImageNet",
        "imagenet_2": "C:\\Users\\Liang\\dataset\\ImageNet",
        "cifar_2": "C:\\Users\\Liang\\dataset\\Cifar10\\Clean"
    }
